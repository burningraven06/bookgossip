<% if @publisher.errors.any? %>
	$("#show_flash_messages").html("<%= j render'common_form_errors/atpublisher_form_errors' %>");
<% elsif not @publisher.errors.any? %>
	$("#publisher_forms").fadeOut(100);
	$("#publisher_<%= @publisher.id %>").fadeOut();
	$("#all_publishers_div").html("<%= j render 'publishers' %>");
	$("#show_flash_messages").html("<%= j render'publishers/publisher_flash_messages/publisher_updated' %>");
	$("#show_all_publishers").fadeIn(100);
<% end %>